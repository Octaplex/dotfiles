super + Return
    urxvt
super + w
    luakit
super + W
    luakit -U -u private
super + f
    urxvt -e zsh -ic ranger
super + m
    urxvt -e zsh -ic ncmpcpp
super + d
    ~/bin/launcher
ctrl + shift + k ; {g,e,a,h,c,j,r}
    setxkbmap {gr,us,ara,il,cn,jp,ru}
ctrl + alt + p
    ~/.config/panel/panel &
ctrl + alt + k
    ~/bin/bindkeys
